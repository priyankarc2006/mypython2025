        "def greet(name):\n",
        "    if name:\n",
        "        print(f\"Hello, {name}!\")\n",
        "    else:\n",
        "        print(\"Hello, stranger!\")\n",
        "\n",
        "greet(\"Alice\")  # Calls the function with a name\n",
        "greet(\"\")       # Calls the function without a name\n"
      ]
